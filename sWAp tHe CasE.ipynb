   "source": [
    "def swap_case(s):\n",
    "    strg=''\n",
    "    for i in s:\n",
    "        if i.upper()!=i:\n",
    "            strg += i.upper()\n",
    "        else:\n",
    "            strg += i.lower()\n",
    "    return strg\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    s = input()\n",
    "    result = swap_case(s)\n",
    "    print(result)"
   ]
  }
