   "source": [
    "def LeapYear(year):\n",
    "    if year%400 == 0 or (year%4 == 0 and year%100 != 0):\n",
    "        return f'{year} IS LEAP YEAR'\n",
    "    else:\n",
    "        return f'{year} IS NOT LEAP YEAR'\n",
    "    \n",
    "if __name__ == '__main__':\n",
    "    YEAR=int(input('Enter the Year: '))\n",
    "    result=LeapYear(YEAR)\n",
    "    print(result)"
   ]
  }
